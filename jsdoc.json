{
	"source": {
		"include": [
			"src/components",
			"src/types"
		],
		"includePattern": ".+\\.(jsx|js)?$"
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/jsdoc-tsimport-plugin/index.js",
		"node_modules/jsdoc-plugin-intersection/plugin.js"
	],
	"opts": {
		"encoding": "utf8",
		"recurse": true,
		"destination": "docs",
		"template": "./node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"homepageTitle": "React Skeleton Docs",
			"title": "React Skeleton Docs",
			"default_theme": "light",
			"includeFilesListInHomepage": true
		}
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"useLongnameInNav": true
		}
	},
	"markdown": {
		"hardwrap": false,
		"idInHeadings": true
	}
}